Algoritmo ObtenerResiduoSinModulo
	Definir num1, num2, divEntera, residuo Como Real;
	//Definir residuo Como Real
    Escribir "Introduce el primer número:";
    Leer num1;
    Escribir "Introduce el segundo número:";
    Leer num2;
    divEntera <- num1 / num2;
    residuo <- num1 - (divEntera * num2);
    Escribir "El residuo de la división es: ", residuo;
FinAlgoritmo
